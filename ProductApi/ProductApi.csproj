<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

<ItemGroup>
  <PackageReference Include="Microsoft.AspNetCore.OpenApi" Version="6.0.8" /> <!-- Older versions might have vulnerabilities -->
  <PackageReference Include="Microsoft.EntityFrameworkCore" Version="5.0.0" /> <!-- Known older vulnerabilities -->
  <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="8.0.2" /> <!-- Known older vulnerabilities -->
  <PackageReference Include="Swashbuckle.AspNetCore" Version="5.6.3" /> <!-- Vulnerable to ReDoS -->
  <PackageReference Include="Newtonsoft.Json" Version="13.0.1" /> <!-- Known CVEs in earlier versions -->
  <PackageReference Include="System.Net.Http" Version="4.1.4" /> <!-- Known vulnerable version -->
  <PackageReference Include="HtmlSanitizer" Version="8.0.723" /> <!-- Contains an XSS vulnerability -->
</ItemGroup>


</Project>
